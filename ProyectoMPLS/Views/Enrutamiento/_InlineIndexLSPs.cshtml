@model ProyectoMPLS.Models.Topologia.InlineLSPViewModel

<div class="list-group">

    @if (Model != null && Model.listaLSPs.Any())
    {
        foreach (var item in Model.listaLSPs)
        {
            <a data-toggle="modal" data-target=".bs-example-modal-lg" href='@Url.Action("_EditarLSP", "Enrutamiento", new { idProyecto = item.idProyecto, idLSP = item.idLSP })' class="list-group-item">
                @Html.DisplayFor(modelItem => item.cNombre)
            </a>
        }
    }
    else
    {
        //Message to display no student record found.
    }


    

    
    
</div>



<a href='@Url.Action("_CrearLSP", "Enrutamiento", new { idProyecto = Model.idProyecto })'
   role="button"
   class="btn btn-default btn-block waves-effect waves-light m-t-10"
   data-toggle="modal" data-target=".bs-example-modal-lg">
    Agregar Ruta Estática
</a>

<a href='@Url.Action("_CrearCSPF", "Enrutamiento", new { idProyecto = Model.idProyecto })'
   role="button"
   class="btn btn-default btn-block waves-effect waves-light m-t-10"
   data-toggle="modal" data-target=".bs-example-modal-lg">
    Agregar por CPSF
</a>


<!--  Modal content for the above example -->
<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->